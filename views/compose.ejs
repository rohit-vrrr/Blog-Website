<%- include("partials/header"); -%>

<h1>Compose</h1>
<form class="" action="/compose" method="post">

  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" name="composeTitle" placeholder="Insert Title Here" autocomplete="off">
  </div>
  <div class="form-group">
    <label>Content</label>
    <textarea class="form-control" rows="6" name="composeContent" placeholder="Insert Text Here" autocomplete="off"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Publish</button>

</form>

<%- include("partials/footer"); -%>
